<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description
    about="urn:mozilla:install-manifest"

    em:name="Vault Shortcut"
    em:description="Adds a toolbar button that opens the LastPass Vault in an external browser."
    em:creator="Isaac Schemm"

    em:id="vault-shortcut@lakora.us"
    em:version="1.0.2"
    em:homepageURL="https://github.com/IsaacSchemm/vault-shortcut"

    em:iconURL="chrome://vault-shortcut/content/icon.png"
	em:optionsURL="chrome://vault-shortcut/content/options.xul"
	em:optionsType="2" >

    <em:targetApplication>
	  <!-- SeaMonkey -->
	  <Description>
		<em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
		<em:minVersion>2.4</em:minVersion>
		<em:maxVersion>*</em:maxVersion>
	  </Description>
	</em:targetApplication>

    <em:targetApplication>
	  <!-- Pale Moon -->
	  <Description>
		<em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
		<em:minVersion>26.5</em:minVersion>
		<em:maxVersion>27.*</em:maxVersion>
	  </Description>
	</em:targetApplication>

  </Description>

</RDF>
